{
  "name": "vue-full-stack-boilerplate",
  "version": "0.0.0",
  "author": "Tom Schweers",
  "private": true,
  "scripts": {
    "start": "nodemon server/bin/www",
    "dev": "npm run dev-nix",
    "dev-nix": "cross-env NODE_ENV=development webpack-dev-server --open --hot --content-base server/public & npm run start",
    "dev-win": "start cross-env NODE_ENV=development webpack-dev-server --open --hot --content-base server/public && npm run start",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "serve": "npm run build && nodemon server/bin/www",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "jade": "~1.11.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.11.0",
    "serve-favicon": "^2.5.0",
    "vue": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "style-loader": "^0.16.1",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
